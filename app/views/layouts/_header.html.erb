<h1><%= link_to 'Private Events', root_path %></h1>

<% if session[:user_id] %>
  <span>Welcome <%= current_user.username %></span>
  <%= link_to 'Dashboard', user_path(session[:user_id]) %>
  <%= link_to 'Log out!', logout_path, method: :delete %>
<% else %>
  <%= link_to "Log in!", login_path %>
<% end %>